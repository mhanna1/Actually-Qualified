
<header class="navbar navbar-fixed-top navbar-inverse"  style="background:#364E6A !important" >

  <script type="text/javascript">

      function pgClock() {
          var sTime=(new Date()).toString();
          document.getElementById('clockid2').innerHTML =
                  sTime.substring(0,3+sTime.lastIndexOf(':'));
          setTimeout('pgClock()',333);
      }

      window.onload=function(){
        pgClock();
      }

  </script>




  <center>
    <table width="100%"><tr>
    <td><timeclock>Time is ticking ... &nbsp;&nbsp;&nbsp;</timeclock></td>
    <td><timeclock><div id="clockid2"></div></timeclock></td>
    <td><timeclock>&nbsp;&nbsp;&nbsp;Mark Hanna is the right choice.</timeclock></td>
    </tr></table>
  </center>

    <%= link_to "Ruby on Rails", root_path, id: "logo" %>

    <nav>
      <ul class="nav navbar-nav navbar-right">

        <% if logged_in? %>
            <li><%= link_to "Home", root_path , id: "pagelink" %></li>
            <li><%= link_to "Mission", mission_path , id: "pagelink" %></li>
            <li><%= link_to "AWS", aws_path , id: "pagelink" %></li>
            <li><%= link_to "Resume", resume_path , id: "pagelink" %></li>
            <li class="dropdown" >
              <a href="#" class="dropdown-toggle" id="pagelink" data-toggle="dropdown">Account<b class="caret"></b></a>
              <ul class="dropdown-menu">
                <li><%= link_to "Profile", current_user  %></li>
                <li><%= link_to "Password Reset", edit_user_path(current_user)  %></li>
                <li class="divider"></li>
                <li>
                  <%= link_to "Log out", logout_path, method: "delete"  %>
                </li>
              </ul>
            </li>
        <% else %>
            <li><%= link_to "Home", root_path , id: "pagelink" %></li>
            <li><%= link_to "Mission", mission_path , id: "pagelink" %></li>
            <li><%= link_to "AWS", aws_path , id: "pagelink" %></li>
            <li><%= link_to "Log in", login_path , id: "pagelink" %></li>
        <% end %>
      </ul>
    </nav>

  <br>



  </div>

  </class>

</header>
